<%- include('index')  %> 

<div class="container-fluid">
    <div class="jumbotron text-center">
      <h1>Formulario de ingreso de productos</h1>
      <a href="http://localhost:8080/productos" type="button" class="btn btn-secondary">Ver productos cargados</a>
    </div>
    <form action="/productos/update/" class="was-validated m-2 w-50 " method="PUT" >
        <div class="form-group p-2">
          <% data.forEach(function(data) { %>
        <input type="number" class="form-control m-2" id="price" placeholder="Enter precio" name="price" required value=<%= data.precio %>>
        <input type="text" class="form-control m-2" id="title" placeholder="Enter title" name="title" required value=<%= data.title %>>
        <input type="text" class="form-control m-2" id="thumbnailUrl" placeholder="Enter thumbnailUrl" name="thumbnailUrl" required value=<%= data.thumbnailUrl %>>
        <input type="number" class="form-control m-2" id="reviews" placeholder="Enter reviews" name="reviews" required value=<%= data.reviews %>>
        <input type="number" class="form-control m-2" id="star" placeholder="Enter star" name="star" required value=<%= data.star %>>
        <input type="text" class="form-control m-2" id="Categoria" placeholder="Enter Categoria" name="Categoria" required value=<%= data.Categoria %>>
        <input type="text" class="form-control m-2" id="description" placeholder="Enter description" name="description" required value=<%= data.description %>>
        <input type="number" class="form-control m-2" id="stock" placeholder="Enter stock" name="stock" required value=<%= data.stock %>>
        <% }); %> 
     
        <button type="submit"  id="button"  class="btn btn-primary m-2">Actualizar</button>
      </form>
    
    </div>